
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option type 'bridge'
	option _orig_ifname 'eth0 wlan0 radio1.network1 wlan2 radio3.network1'
	option _orig_bridge 'true'
	option ifname 'eth0'
	option proto 'dhcp'

config globals 'globals'
	option ula_prefix 'fda6:71b7:5aa5::/48'

config interface 'lan_24G'
	option type 'bridge'
	option proto 'dhcp'

config interface 'lan_5G'
	option type 'bridge'
	option proto 'dhcp'


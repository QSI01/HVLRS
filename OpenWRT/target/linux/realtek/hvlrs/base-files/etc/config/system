config system
	option hostname	HVLRS
	option timezone Japan
	option zonename Japan
	option log_file /var/log/messages
	option log_buffer_size 16

config timeserver ntp
	list server	0.openwrt.pool.ntp.org
	list server	1.openwrt.pool.ntp.org
	list server	2.openwrt.pool.ntp.org
	list server	3.openwrt.pool.ntp.org
	option enabled 1
	option enable_server 0

config 'led'
	option 'sysfs'		'IODATA:R'
        option 'trigger'	'none'
	option 'brightness'	'255'
        option 'delayon'        '200'
        option 'delayoff'       '200'

config 'led'
	option 'sysfs'		'IODATA:G'
        option 'trigger'	'none'
	option 'brightness'     '255'
	option 'delayon'	'200'
	option 'delayoff'	'200'

config 'led' 
        option 'sysfs'          'IODATA:B'
        option 'trigger'        'none'
	option 'brightness'     '255'
        option 'delayon'        '200'
        option 'delayoff'       '200'


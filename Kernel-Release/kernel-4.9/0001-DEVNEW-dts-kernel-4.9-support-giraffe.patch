diff --git a/kernel/arch/arm64/boot/dts/realtek/rtd129x/rtd-1295-nas-qa-rescue.dts b/kernel/arch/arm64/boot/dts/realtek/rtd129x/rtd-1295-nas-qa-rescue.dts
